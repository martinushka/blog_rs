<% @article.each do |article| %>
	<h1> <%= article.title %> </h1>
	<p> <%= article.text %> </p>
	<%= link_to 'Show', article_path(article) %> |
	<%= link_to 'Edit', edit_article_path(article) %> |
	<%= link_to "Destroy", article_path(article), method: :delete, data: { confirm: 'Действительно удалить?'} %>
	<hr>
<% end %>